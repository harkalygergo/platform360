{% extends 'platform/dashboard/_main.html.twig' %}

{% block title %}Add Block{% endblock %}

{% block main %}
    <h1>Add Block</h1>

    {{ form_start(form) }}
        {{ form_row(form.name) }}
        {{ form_row(form.content) }}
        {{ form_row(form.status) }}
        {{ form_row(form.save) }}
    {{ form_end(form) }}

    <p><a href="{{ path('web_block_index') }}">Back to blocks</a></p>
{% endblock %}

